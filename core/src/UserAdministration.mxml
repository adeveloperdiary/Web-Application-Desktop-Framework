<?xml version="1.0" encoding="utf-8"?>
<PanelWindow xmlns="com.ajana.panel.*" xmlns:mx="http://www.adobe.com/2006/mxml" 
	width="800" height="364" initialize="init()"
	horizontalAlign="center" verticalAlign="top" enableResize="true" hideEffect="{fadeOut}"
	showEffect="{fadeIn}" x="60" y="60"  enableZoom="true">
	<mx:Fade id="fadeOut" duration="500" alphaFrom="1.0" alphaTo="0.0"/>
	<mx:Fade id="fadeIn" duration="500" alphaFrom="0.0" alphaTo="1.0"/>
	
	
	<mx:Script>
		<![CDATA[
			
			public var windowTitle:String;
			
			private function init():void {
				this.title="User Administration";
			}
			
		]]>
	</mx:Script>
	<mx:VBox height="100%" width="100%">
		<mx:HBox width="100%" height="28">
		</mx:HBox>
		<mx:HBox width="100%" height="100%">
			<mx:VBox height="100%" width="100%">
				<mx:HBox width="100%">
					<mx:Spacer width="10"/>
					<mx:ApplicationControlBar styleName="ApplicationControlBarSkin" width="100%" paddingLeft="10" paddingRight="10" dock="true">
						<mx:CheckBox label="Checkbox"/>
						<mx:DateField/>
						<mx:RadioButtonGroup id="radiogroup1"/>
						<mx:RadioButton label="Button 1" groupName="radiogroup1"/>
						<mx:RadioButton label="Button 2" groupName="radiogroup1"/>
						<mx:ComboBox></mx:ComboBox>
						<mx:Button label="Button" />
					</mx:ApplicationControlBar>
					<mx:Spacer width="10"/>
				</mx:HBox>
				<mx:HBox width="100%" height="50%">
					<mx:Spacer width="10"/>
					<Fieldset width="45%" height="100%" paddingLeft="10">
					</Fieldset>
					<mx:Spacer width="10%"/>
					<Fieldset width="45%" height="100%" paddingRight="10">
					</Fieldset>
					<mx:Spacer width="10"/>
				</mx:HBox>
				<mx:HBox width="100%" height="50%">
					<mx:Spacer width="10"/>
					<mx:VBox height="100%" width="100%">
						<mx:Spacer width="10"/>
						<mx:TextArea width="100%" height="100%"/>
						<mx:Spacer width="10"/>	
					</mx:VBox>
					<mx:Spacer width="10"/>
				</mx:HBox>
			</mx:VBox>
		</mx:HBox>
	</mx:VBox>
	
	
	
</PanelWindow>
