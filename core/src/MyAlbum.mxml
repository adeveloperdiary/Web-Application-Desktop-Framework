<?xml version="1.0" encoding="utf-8"?>
<PanelWindow xmlns="com.met.panel.*" xmlns:mx="http://www.adobe.com/2006/mxml" width="356" height="360"
	showMin="true" showClose="true" hideEffect="{fadeOut}"	showEffect="{fadeIn}" title="Album" x="50" y="50"
	showMax="false" enableZoom="true">
	<mx:Fade id="fadeOut" duration="500" alphaFrom="1.0" alphaTo="0.0"/>
	<mx:Fade id="fadeIn" duration="500" alphaFrom="0.0" alphaTo="1.0"/>

	<mx:VBox height="27" width="100%">
	</mx:VBox>
	<mx:VBox height="100%" width="100%">
		<mx:Spacer width="100%" height="5"/>
			<mx:HBox width="100%" height="100%" >
				<mx:Spacer width="5" height="100%"/>
				<Fieldset width="100%" height="100%">
					<mx:VBox height="100%" width="100%">
						<mx:VBox height="15" width="100%">
						</mx:VBox>
						<mx:HBox width="100%" height="100%" >
							<mx:Spacer width="5" height="100%"/>
							<mx:Grid width="100%" height="100%">
								<mx:GridRow width="100%" height="100%">
									<mx:GridItem width="100%" height="100%" horizontalAlign="center" verticalAlign="middle" borderStyle="solid" borderThickness="1">
										<mx:Image width="128" height="128" source="assets/A1.png" scaleContent="true" autoLoad="true"/>
									</mx:GridItem>
									<mx:GridItem width="100%" height="100%" horizontalAlign="center" verticalAlign="middle" borderStyle="solid" borderThickness="1">
										<mx:Image width="128" height="128" source="assets/A2.png" scaleContent="true" autoLoad="true"/>
									</mx:GridItem>
								</mx:GridRow>
								<mx:GridRow width="100%" height="100%">
									<mx:GridItem width="100%" height="100%" horizontalAlign="center" verticalAlign="middle" borderStyle="solid" borderThickness="1">
										<mx:Image width="128" height="128" source="assets/A3.png" scaleContent="true" autoLoad="true"/>
									</mx:GridItem>
									<mx:GridItem width="100%" height="100%" horizontalAlign="center" verticalAlign="middle" borderStyle="solid" borderThickness="1">
										<mx:Image width="128" height="128" source="assets/A4.png" scaleContent="true" autoLoad="true"/>
									</mx:GridItem>
								</mx:GridRow>
							</mx:Grid>
							<mx:Spacer width="5" height="100%"/>
						</mx:HBox>
						<mx:Spacer width="100%" height="5"/>
					</mx:VBox>
				</Fieldset>
				<mx:Spacer width="5" height="100%"/>
			</mx:HBox>
		<mx:Spacer width="100%" height="5"/>
	</mx:VBox>
</PanelWindow>
